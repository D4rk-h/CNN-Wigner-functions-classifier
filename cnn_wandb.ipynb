    "class WignerTrainer:\n",
    "    def __init__(self, model, device='cuda' if torch.cuda.is_available() else 'cpu', use_wandb=True):\n",
    "        self.model = model.to(device)\n",
    "        self.device = device\n",
    "        self.use_wandb = use_wandb\n",
    "        self.history = {\n",
    "            'train_loss': [], 'train_acc': [],\n",
    "            'val_loss': [], 'val_acc': []\n",
    "        }\n",
    "        \n",
    "    def train_epoch(self, dataloader, criterion, optimizer, epoch):\n",
    "        self.model.train()\n",
    "        running_loss = 0.0\n",
    "        correct = 0\n",
    "        total = 0\n",
    "        pbar = tqdm(dataloader, desc='Training')\n",
    "        for batch_idx, (images, labels) in enumerate(pbar):\n",
    "            images, labels = images.to(self.device), labels.to(self.device)\n",
    "            optimizer.zero_grad()\n",
    "            outputs = self.model(images)\n",
    "            loss = criterion(outputs, labels)\n",
    "            loss.backward()\n",
    "            optimizer.step()\n",
    "            running_loss += loss.item() * images.size(0)\n",
    "            _, predicted = torch.max(outputs, 1)\n",
    "            total += labels.size(0)\n",
    "            correct += (predicted == labels).sum().item()\n",
    "            \n",
    "            # Log batch metrics to wandb\n",
    "            if self.use_wandb and batch_idx % 10 == 0:\n",
    "                wandb.log({\n",
    "                    'train/batch_loss': loss.item(),\n",
    "                    'train/batch_acc': 100. * (predicted == labels).sum().item() / labels.size(0),\n",
    "                    'epoch': epoch\n",
    "                })\n",
    "            \n",
    "            pbar.set_postfix({'loss': f'{loss.item():.4f}', 'acc': f'{100.*correct/total:.2f}%'})\n",
    "        epoch_loss = running_loss / total\n",
    "        epoch_acc = 100. * correct / total\n",
    "        return epoch_loss, epoch_acc\n",
    "    \n",
    "    def validate(self, dataloader, criterion, epoch, dataset=None):\n",
    "        self.model.eval()\n",
    "        running_loss = 0.0\n",
    "        correct = 0\n",
    "        total = 0\n",
    "        all_preds = []\n",
    "        all_labels = []\n",
    "        sample_images = []\n",
    "        sample_labels = []\n",
    "        sample_preds = []\n",
    "        \n",
    "        with torch.no_grad():\n",
    "            for batch_idx, (images, labels) in enumerate(tqdm(dataloader, desc='Validation')):\n",
    "                images, labels = images.to(self.device), labels.to(self.device)\n",
    "                outputs = self.model(images)\n",
    "                loss = criterion(outputs, labels)\n",
    "                running_loss += loss.item() * images.size(0)\n",
    "                _, predicted = torch.max(outputs, 1)\n",
    "                total += labels.size(0)\n",
    "                correct += (predicted == labels).sum().item()\n",
    "                \n",
    "                all_preds.extend(predicted.cpu().numpy())\n",
    "                all_labels.extend(labels.cpu().numpy())\n",
    "                \n",
    "                # Collect some sample images for wandb logging\n",
    "                if self.use_wandb and batch_idx == 0 and len(sample_images) < 8:\n",
    "                    sample_images.extend(images[:8].cpu())\n",
    "                    sample_labels.extend(labels[:8].cpu().numpy())\n",
    "                    sample_preds.extend(predicted[:8].cpu().numpy())\n",
    "        \n",
    "        epoch_loss = running_loss / total\n",
    "        epoch_acc = 100. * correct / total\n",
    "        \n",
    "        # Log sample predictions to wandb\n",
    "        if self.use_wandb and len(sample_images) > 0 and dataset is not None:\n",
    "            wandb_images = []\n",
    "            for img, true_label, pred_label in zip(sample_images, sample_labels, sample_preds):\n",
    "                img_np = img.squeeze().numpy()\n",
    "                true_class = dataset.get_class_name(true_label)\n",
    "                pred_class = dataset.get_class_name(pred_label)\n",
    "                caption = f\"True: {true_class} | Pred: {pred_class}\"\n",
    "                wandb_images.append(wandb.Image(img_np, caption=caption))\n",
    "            \n",
    "            wandb.log({\n",
    "                'val/predictions': wandb_images,\n",
    "                'epoch': epoch\n",
    "            })\n",
    "        \n",
    "        return epoch_loss, epoch_acc\n",
    "    \n",
    "    def fit(self, train_loader, val_loader, epochs=50, lr=0.001, weight_decay=1e-4, \n",
    "            patience=10, save_path='best_model.pth', dataset=None, project_name='wigner-classification'):\n",
    "        \n",
    "        # Initialize wandb\n",
    "        if self.use_wandb:\n",
    "            wandb.init(\n",
    "                project=project_name,\n",
    "                config={\n",
    "                    'epochs': epochs,\n",
    "                    'batch_size': train_loader.batch_size,\n",
    "                    'learning_rate': lr,\n",
    "                    'weight_decay': weight_decay,\n",
    "                    'patience': patience,\n",
    "                    'optimizer': 'Adam',\n",
    "                    'architecture': 'WignerClassifier',\n",
    "                    'num_classes': len(dataset.class_names) if dataset else 10,\n",
    "                    'device': str(self.device)\n",
    "                }\n",
    "            )\n",
    "            wandb.watch(self.model, log='all', log_freq=100)\n",
    "        \n",
    "        criterion = nn.CrossEntropyLoss()\n",
    "        optimizer = optim.Adam(self.model.parameters(), lr=lr, weight_decay=weight_decay)\n",
    "        scheduler = optim.lr_scheduler.ReduceLROnPlateau(optimizer, mode='min', factor=0.5, patience=5)\n",
    "        best_val_loss = float('inf')\n",
    "        patience_counter = 0\n",
    "        print(f\"\\nTraining on {self.device}\")\n",
    "        print(f\"{'Epoch':<6} {'Train Loss':<12} {'Train Acc':<12} {'Val Loss':<12} {'Val Acc':<12}\")\n",
    "        print(\"-\" * 60)\n",
    "        \n",
    "        for epoch in range(epochs):\n",
    "            train_loss, train_acc = self.train_epoch(train_loader, criterion, optimizer, epoch)\n",
    "            val_loss, val_acc = self.validate(val_loader, criterion, epoch, dataset)\n",
    "            \n",
    "            self.history['train_loss'].append(train_loss)\n",
    "            self.history['train_acc'].append(train_acc)\n",
    "            self.history['val_loss'].append(val_loss)\n",
    "            self.history['val_acc'].append(val_acc)\n",
    "            \n",
    "            # Log epoch metrics to wandb\n",
    "            if self.use_wandb:\n",
    "                wandb.log({\n",
    "                    'epoch': epoch,\n",
    "                    'train/loss': train_loss,\n",
    "                    'train/accuracy': train_acc,\n",
    "                    'val/loss': val_loss,\n",
    "                    'val/accuracy': val_acc,\n",
    "                    'learning_rate': optimizer.param_groups[0]['lr']\n",
    "                })\n",
    "            \n",
    "            print(f\"{epoch+1:<6} {train_loss:<12.4f} {train_acc:<12.2f} \"\n",
    "                  f\"{val_loss:<12.4f} {val_acc:<12.2f}\")\n",
    "            \n",
    "            scheduler.step(val_loss)\n",
    "            \n",
    "            if val_loss < best_val_loss:\n",
    "                best_val_loss = val_loss\n",
    "                patience_counter = 0\n",
    "                torch.save({\n",
    "                    'epoch': epoch,\n",
